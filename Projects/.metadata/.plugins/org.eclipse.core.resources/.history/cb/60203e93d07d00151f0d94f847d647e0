enum Sex { MALE, FEMALE };
enum Colour { WHITE, BROWN, BLACK, SPOTTED};
enum NAME { THUMPER, OREO, LOLO, PEPPER, SMOKEY, PEANUT};
class Bunny {
	public:

	private;
	Sex sex;
	Colour colour;
	NAME name;
	Bunny *next;
	Bunny *mother;
};


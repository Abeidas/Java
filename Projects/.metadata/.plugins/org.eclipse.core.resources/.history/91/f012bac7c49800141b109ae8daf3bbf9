package Tree;

public class Testing {
	public static void main(String args[]) {
		BinarySearchTree<Integer,String> test = new BinarySearchTree<Integer, String>();
		test.insert(50, "lotfi");
		test.insert(50, "fat7i");
		test.insert(20, "hosni");
		
		test.insert(80, "fikri");
		test.insert(70, "sub7i");
		test.insert(15, "mad7i");
		test.insert(30, "hosnie");
		test.insert(90, "boosni");
		System.out.println(test.root.getData()+":" + test.root.getValue());
		System.out.println(test.root.getRightchild().getData()+":" +test.root.getRightchild().getValue());
		System.out.println(test.root.getLeftchild().getData()+":" +test.root.getLeftchild().getValue());
		System.out.println(test.root.getRightchild().getRightchild().getData()+":" +test.root.getRightchild().getRightchild().getValue());
		System.out.println(test.root.getRightchild().getLeftchild().getData()+":" +test.root.getRightchild().getLeftchild().getValue());
		System.out.println(test.root.getLeftchild().getRightchild().getData()+":" +test.root.getLeftchild().getRightchild().getValue());
		System.out.println(test.root.getLeftchild().getLeftchild().getData()+":" +test.root.getLeftchild().getLeftchild().getValue());
		System.out.println(test.search(50));
	}
}